<script type="text/javascript" src="http://www.xbmiaomu.com/api/geetest/js/gt.js"></script>
<script>
var embed_true = false;
    var handlerEmbed = function (captchaObj) {
        $("#embed-submit").click(function (e) {
            var validate = captchaObj.getValidate();
            if (!validate) {
                Dmsg('请先完成验证', 'captcha');
return false;
                e.preventDefault();
            }
        });
        // 将验证码加到id为captcha的元素里，同时会有三个input的值：geetest_challenge, geetest_validate, geetest_seccode
        captchaObj.appendTo("#embed-captcha");
        captchaObj.onReady(function () {
            $("#wait")[0].className = "hide";
        });
        // 更多接口参考：http://www.geetest.com/install/sections/idx-client-sdk.html
    };
    $.ajax({
        // 获取id，challenge，success（是否启用failback）
        url: "http://www.xbmiaomu.com/ajax.php?action=geetest&step=1&t=" + (new Date()).getTime(), // 加随机数防止缓存
        type: "get",
        dataType: "json",
        success: function (data) {
            console.log(data);
            // 使用initGeetest接口
            // 参数1：配置参数
            // 参数2：回调，回调的第一个参数验证码对象，之后可以使用它做appendTo之类的事件
            initGeetest({
                gt: data.gt,
                challenge: data.challenge,
                new_captcha: data.new_captcha,
                product: "embed", // 产品形式，包括：float，embed，popup。注意只对PC版验证码有效
                offline: !data.success // 表示用户后台检测极验服务器是否宕机，一般不需要关注
                // 更多配置参数请参见：http://www.geetest.com/install/sections/idx-client-sdk.html#config
            }, handlerEmbed);
        }
    });
</script>
<script type="text/javascript">
function Scheck() {
if(Dd('mobile').value.length < 11) {
Dmsg('请输入手机号码', 'mobile');
return false;
}
if(Dd('code').value.length < 6) {
Dmsg('请输入短信验证码', 'code');
return false;
}
if(!Dd('read').checked) {
Dmsg('请阅读并同意服务条款', 'read');
return false;
}
return true;
}
function Sstop() {
var i = 180;
var interval=window.setInterval(
function() {
if(i == 0) {
$('#send').html('发送短信');
$('#sendok').html('');
clearInterval(interval);
} else {
$('#send').html('重新发送('+i+'秒)');
i--;
}
},
1000);
}
function Ssend() {
var geetest_challenge_val = $("input[name='geetest_challenge']").val();
var geetest_validate_val = $("input[name='geetest_validate']").val();
var geetest_seccode_val = $("input[name='geetest_seccode']").val();
if($('#send').html().indexOf('秒') != -1) {
Dmsg('请耐心等待', 'send');
return false;
}
if(Dd('mobile').value.length < 11) {
Dmsg('请输入手机号码', 'mobile');
return false;
}
if (!geetest_challenge_val || !geetest_validate_val || !geetest_seccode_val) {
Dmsg('请先完成验证', 'captcha');
return false;
}
// if($('#ccaptcha').html().indexOf('ok.png') == -1) {
// console.log(122)
// Dmsg('请填写验证码', 'captcha');
// return false;
// }
$.post('?', 'action=sendsms&sid=954455e34fd5219e61c7abdaa1ca0509&mobile='+Dd('mobile').value+'&geetest_challenge='+geetest_challenge_val+'&geetest_validate='+geetest_validate_val+'&geetest_seccode='+geetest_seccode_val, function(data) {
if(data == 'ok') {
$('#sendok').html('短信发送成功');
Sstop();return;
} else if(data == 'format') {
Dmsg('手机格式错误', 'mobile');
} else if(data == 'captcha') {
Dmsg('验证码错误', 'code');
} else if(data == 'exist') {
Dmsg('手机号码已经被注册', 'mobile');
} else if(data == 'max') {
Dmsg('发送次数过多', 'code');
} else if(data == 'fast') {
Dmsg('发送频率过快', 'code');
} else {
Dmsg('发送失败'+data, 'code');
}
// reloadcaptcha();
});
}
</script>
